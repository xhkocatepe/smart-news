{
  "name": "smart-news",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www",
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "prod": "NODE_ENV=production nodemon ./bin/www",
    "test": "jest -c jest.config.js --passWithNoTests --runInBand --detectOpenHandles --forceExit",
    "unit": "jest -c jest.config.unit.js --passWithNoTests --runInBand --detectOpenHandles --forceExit",
    "integration": "jest -c jest.config.integration.js --passWithNoTests --runInBand --detectOpenHandles --forceExit",
    "config": "node ./scripts/configure.js",
    "deconfig": "node ./scripts/deconfigure.js",
    "package-deploy": "npm run package && npm run deploy"
  },
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "axios": "^0.20.0",
    "bcrypt": "^3.0.6",
    "compare-versions": "^3.6.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.0.0-rc.1",
    "ejs": "^3.1.5",
    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "kue-scheduler": "^0.10.13",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.4.9",
    "morgan": "~1.9.0",
    "pluralize": "^7.0.0",
    "redis": "^3.0.2",
    "redis-server": "^1.2.2",
    "rss-parser": "^3.9.0",
    "swagger-ui-express": "^4.1.5",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "bcrypt": "^3.0.6",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-environment-node": "^24.8.0",
    "kue-scheduler": "^0.10.13",
    "lint-diff": "^1.2.1",
    "mongodb-memory-server": "^5.1.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.16.3",
    "supertest": "^3.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-diff"
    }
  }
}
